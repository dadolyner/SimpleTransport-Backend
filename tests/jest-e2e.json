{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
        "<rootDir>"
    ],
    "moduleDirectories": [
        "<rootDir>/../",
        "node_modules"
    ],
    "testRegex": "spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
}